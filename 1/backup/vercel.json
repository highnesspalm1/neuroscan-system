{
  "version": 2,
  "buildCommand": "cd WebFrontend && npm install && npm run build",
  "outputDirectory": "WebFrontend/dist",
  "installCommand": "cd WebFrontend && npm install",
  "framework": null,
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*\\.(js|css|svg|png|jpg|jpeg|gif|ico|woff|woff2|ttf|eot))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://neuroscan-api.onrender.com",
    "VITE_ENVIRONMENT": "production"
  }
}
